{
  "name": "harmonix-pro-analyzer",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "predev": "bash scripts/copy-essentia.sh",
    "dev": "vite",
    "prebuild": "npm run clean && npm run typecheck && bash scripts/copy-essentia.sh",
    "build": "tsc -b && vite build",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist",
    "build:prod": "cross-env NODE_ENV=production npm run build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 200",
    "lint:fix": "npm run lint -- --fix",
    "typecheck": "tsc --noEmit",
    "test": "npm run typecheck && npm run lint",
    "test:integration": "node test-integration.js",
    "preview": "vite preview",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^4.15.0",
    "@types/audioworklet": "^0.0.50",
    "essentia.js": "^0.1.3",
    "lucide-react": "^0.263.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.19.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "postcss": "^8.4.41",
    "rimraf": "^5.0.5",
    "rollup-plugin-visualizer": "^5.12.0",
    "tailwindcss": "^3.4.10",
    "terser": "^5.31.0",
    "typescript": "^5.2.2",
    "vite": "^5.3.4",
    "vite-bundle-analyzer": "^0.7.0",
    "vite-plugin-pwa": "^0.20.0",
    "workbox-window": "^7.1.0"
  }
}
